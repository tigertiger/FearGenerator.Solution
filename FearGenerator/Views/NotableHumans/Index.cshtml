@{
  Layout = "_Layout";
}

@using FearGenerator.Models;
@model List<FearGenerator.Models.NotableHuman>;

<div class="container">
  <span>&nbsp;</span>
  <section class=survival>
    <h1 class="survival bleeding">Notable Personages</h1>

    @if (Model.Count == 0)
    {
      <h3>TBD</h3>
    }

    <ul>
      @foreach(NotableHuman notableHuman in Model)
      {
        <li>@notableHuman.HumanName</li>
      }
    </ul>

    <div class="bottomOpts">
      <p>@Html.ActionLink("Back to Home", "Index", "Home")</p>
    </div>
  </section>
</div>